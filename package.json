{
  "name": "sparkles-component",
  "version": "1.1.0",
  "description": "Experiment with `@glimmer/component` style APIs in Ember apps via existing public APIs",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "Robert Jackson <me@rwjblue.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/rwjblue/sparkles-component",
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
